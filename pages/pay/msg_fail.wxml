      <view class="weui-msg__title">支付失败</view>
      <view class="weui-msg__desc">{{msg}}</view>
    </view>
    <view class="weui-msg__opr-area">
      <view class="weui-btn-area">
        <navigator open-type='navigateBack'>
          <button class="weui-btn" type="primary">返回</button>
        </navigator>

        <!-- <button class="weui-btn" type="primary">返回</button> -->
        <!-- <button class="weui-btn" type="default">辅助操作</button> -->
      </view>
    </view>
    <view class="weui-msg__extra-area">
      <view class="weui-footer">
        <view class="weui-footer__links">
          <navigator url="" class="weui-footer__link">i会员</navigator>
        </view>
        <!-- <view class="weui-footer__text">Copyright © 2008-2016 weui.io</view> -->
      </view>
    </view>
